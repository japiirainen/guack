{
   "name": "guack",
   "version": "1.0.0",
   "main": "index.js",
   "repository": "https://github.com/japiirainen/guack.git",
   "author": "Joona Piirainen <joona.piirainen@gmail.com>",
   "license": "MIT",
   "dependencies": {
      "@types/lodash": "^4.14.164",
      "lodash": "^4.17.20",
      "uuid": "^8.3.0"
   },
   "devDependencies": {
      "@types/jest": "^26.0.15",
      "@types/uuid": "^8.3.0",
      "cypress": "6.8.0",
      "husky": "4",
      "jest": "^26.6.3",
      "lint-staged": "^10.5.4",
      "npm-run-all": "^4.1.5",
      "prettier": "^2.2.1",
      "ts-jest": "^26.4.3",
      "typescript": "^4.0.2"
   },
   "private": true,
   "engines": {
      "node": "^15"
   },
   "workspaces": [
      "amole"
   ],
   "scripts": {
      "start:dev": "npm-run-all --parallel start:db start:dev:backend",
      "start:dev:no-db": "npm-run-all --parallel watch:frontend start:dev:backend",
      "start:db": "docker-compose up||true",
      "watch:frontend": "yarn --cwd frontend watch",
      "start:dev:backend": "yarn --cwd amole start:dev",
      "start": "yarn start:amole",
      "start:backend": "yarn --cwd amole start",
      "build": "yarn build:frontend && yarn build:amole",
      "build:frontend": "yarn --cwd frontend build",
      "build:backend": "yarn --cwd amole build",
      "test-unit": "npm-run-all --parallel test-unit-common",
      "prettier:check": "prettier --check .",
      "check-sources": "yarn prettier:check && yarn --cwd backend check-sources",
      "typecheck:dev:frontend": "cd frontend && tsc --noEmit --preserveWatchOutput --watch",
      "format": "prettier --write ."
   }
}
